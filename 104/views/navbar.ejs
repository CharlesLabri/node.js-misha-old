
<% var links = [
	{name: "Administrative", path: "/administrative/"},
	{name: "Sales", path : "/sales/"},
	{name: "FI", path : "/fi/"},
	{name: "Parts", path : "/parts/"},
	{name: "Service", path : "/service/"},
	{name: "IT", path : "/it/"}
	] %>


<div class="navbar navbar-inverse navbar-fixed-top">
	<div class="navbar-inner">
		<div class="container-fluid">
			<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</a>
			<a class="brand" href="/">Misha</a>
			<div class="nav-collapse collapse">            
				<ul class="nav">
   					<% links.forEach(function(link){ %>
					<% if (link.name == active) { %>
						<li class="dropdown active">
					<% }else { %>
						<li class="dropdown">
					<% } %>	
							<a class="dropdown-toggle" data-toggle="dropdown" href="#"><%= link.name %><b class="caret"></b></a>
							<ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
								<li><a href="<%=link.path%>links">Links</a></li>
								<li><a href="<%=link.path%>docs">Documents</a></li>
								<% if (link.name == "IT") { %>
									<li><a href="<%=link.path%>ticket">Tickets</a></li>
									<li><a href="<%=link.path%>contact">Contact</a></li>
									<li><a href="<%=link.path%>training">Training</a></li>
									<li><a href="<%=link.path%>zerotolerance">ZeroTolerance</a></li>
								<% } %>
							</ul>
						</li>
					<% }) %>					
				</ul>           
			</div><!--/.nav-collapse -->
		</div>
	</div>
</div>
<script>
	$('body').on('touchstart.dropdown', '.dropdown-menu', function (e) { e.stopPropagation(); });
</script>
